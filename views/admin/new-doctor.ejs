<%- include('./common/header.ejs'); %>
<div class="card card-primary">
    <div class="card-header">
        <h3 class="card-title">New Counsellor Form</h3>

        <div class="card-tools">
            <button type="button" class="btn btn-tool" data-card-widget="collapse">
                <i class="fas fa-minus"></i>
            </button>
        </div>
    </div>
    <div class="card-body">
        <div class="form-group col-md-6">
            <label for="exampleInputEmail1">Counsellor's Phone Number</label>
            <input type="number" class="form-control" id="doctorNumber"
                placeholder="Enter Doctor's Phone Number" required>
        </div>
        <div class="form-group col-md-6">
            <label for="exampleInputEmail1">Counsellor's Name</label>
            <input type="text" class="form-control" id="doctorName"
                placeholder="Enter Doctor's Name" required>
        </div>
    </div>
    <div class="card-footer">
        <button type="submit" class="btn btn-primary" id="book-appointment">Register New Counsellor</button>
    </div>
</div>


